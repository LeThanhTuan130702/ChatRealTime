@{
}
@model ICollection<ChatRealTime.Models.FriendVM>
<div class="users-list ">
    @foreach(var fr in Model)
    {
        <a asp-action="Chat" asp-controller="Home" asp-route-id="@fr.Id">
            <div class="content">
                <img src="~/Hinh/Image/@fr.img">
                <div class="details">
                    <span>@fr.Username</span>
                    <p>@fr.message
                </div>
            </div>
            @if(fr.status==0)
            {
                <div class="status-dot offline"><i class="fas fa-circle"></i></div>

            }
            else
            {
                <div class="status-dot "><i class="fas fa-circle"></i></div>

            }
        </a>
    }
